      "advisor_enabled": true,
      "honeypot": true,
      "threat_level": "auto"
    },
    "security": {
      "key_rotation_days": 7,
      "max_login_attempts": 5,
      "session_duration": 24,
      "paranoia_mode": false
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "server.js",
      "base/**/*.js",
      "scripts/**/*.js",
      "!**/node_modules/**",
      "!**/tests/**",
      "!**/coverage/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/coverage/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.py": [
      "autopep8 --in-place --aggressive --aggressive"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:encryption",
      "pre-push": "npm test && npm run security:audit && npm run encryption:verify"
