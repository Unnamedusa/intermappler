    "ip": "^1.1.8",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/private/intermapper-v8.git"
  },
  "bugs": {
    "url": "https://github.com/private/intermapper-v8/issues"
  },
