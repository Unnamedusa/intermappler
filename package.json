{
  "name": "intermapper-v8",
  "version": "8.0.0",
  "private": true,
  "description": "INTERMAPPLER v8.0 - Sistema Personal con Acceso Restringido por IP",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:ip": "cross-env PORT=3000 nodemon server.js",
    "start:custom": "cross-env PORT=3001 node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "railway up",
    "logs": "railway logs",
    "status": "railway status",
    "connect": "railway connect 3000",
    "myip": "node -e \"require('https').get('https://api.ipify.org?format=json', (r) => {let d='';r.on('data',c=>d+=c);r.on('end',()=>console.log('Tu IP pÃºblica:', JSON.parse(d).ip))})\"",
    "setup": "npm run myip && echo 'Actualiza YOUR_IP_ADDRESS en server.js con la IP mostrada arriba'",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "security-check": "node security-check.js"
  },
  "keywords": [
    "geospatial",
    "mapping",
    "intelligence",
    "security",
    "private-access",
    "ip-restricted"
  ],
  "author": "INTERMAPPLER Security Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/private/intermapper-v8.git"
  },
  "bugs": {
    "url": "https://github.com/private/intermapper-v8/issues"
  },
  "homepage": "https://github.com/private/intermapper-v8#readme",
  "files": [
    "server.js",
    "index.html",
    "package.json",
    "README.md",
    ".env.example"
  ],
  "config": {
    "port": 3000,
    "allowed_ip": "TU_IP_PUBLICA_AQUI"
  }
}