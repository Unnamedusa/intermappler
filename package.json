    "stats:system": "node scripts/system-stats.js",
    "backup": "node scripts/backup.js",
    "backup:encrypted": "node scripts/backup.js --encrypt --layers=3",
    "backup:full": "node scripts/backup.js --full --encrypt --compress",
    "clean": "rm -rf node_modules coverage .nyc_output && rm -f package-lock.json",
    "clean:cache": "rm -rf .cache && rm -f base/incript/*.cache && rm -f logs/*.log",
    "docker:build": "docker build -t intermappler:v3 .",
    "docker:run": "docker run -p 3000:3000 -e NODE_ENV=production intermappler:v3",
    "docker:run:secure": "docker run -p 3000:3000 -e ENCRYPTION_LAYERS=3 -e PARANOIA_MODE=true intermappler:v3",
    "docker:compose": "docker-compose up -d",
    "docker:compose:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "pm2:start": "pm2 start server.js --name intermappler --watch",
    "pm2:start:secure": "pm2 start server.js --name intermappler-secure -- --encryption-layers=3",
    "pm2:stop": "pm2 stop intermappler",
    "pm2:delete": "pm2 delete intermappler",
    "pm2:restart": "pm2 restart intermappler",
    "pm2:logs": "pm2 logs intermappler",
    "pm2:monitor": "pm2 monit",
    "pm2:status": "pm2 status",
    "quantum:init": "node scripts/init-quantum.js",
    "quantum:status": "node scripts/quantum-status.js",
    "quantum:reset": "node scripts/reset-quantum.js",
    "enigma:rotate": "node scripts/rotate-enigma-keys.js",
    "enigma:status": "node scripts/enigma-status.js",
    "enigma:reset": "node scripts/reset-enigma.js",
    "sneaker:activate": "node scripts/activate-sneaker.js",
    "sneaker:deactivate": "node scripts/deactivate-sneaker.js",
    "sneaker:status": "node scripts/check-sneaker-status.js",
    "sneaker:advisor": "python3 base/incript/advisor.py",
    "sneaker:test": "node scripts/test-sneaker.js",
    "layers:status": "node scripts/check-layers-status.js",
    "layers:enable": "node scripts/enable-layers.js --all",
    "layers:disable": "node scripts/disable-layers.js --all",
    "layers:test": "node scripts/test-all-layers.js",
    "layers:benchmark": "node scripts/benchmark-layers.js",
    "system:diagnose": "node scripts/diagnose-system.js",
    "system:encrypt": "node scripts/system-encrypt.js --data 'test' --layers 3",
    "system:decrypt": "node scripts/system-decrypt.js --file encrypted.json",
    "system:health": "node scripts/health-check.js",
    "system:monitor": "node scripts/monitor-system.js",
    "keys:generate": "node scripts/generate-all-keys.js",
    "keys:backup": "node scripts/backup-keys.js --encrypt",
    "keys:restore": "node scripts/restore-keys.js",
    "logs:view": "tail -f logs/server.log",
    "logs:security": "tail -f logs/security.log",
    "logs:encryption": "tail -f logs/encryption.log",
    "logs:clear": "> logs/server.log && > logs/security.log && > logs/encryption.log",
    "api:test": "node scripts/test-api.js",
    "api:stress": "node scripts/stress-test.js",
    "api:docs": "node scripts/generate-docs.js",
    "security:scan": "node scripts/security-scan.js",
    "security:harden": "node scripts/harden-security.js",
    "update:check": "node scripts/check-updates.js",
    "update:encryption": "node scripts/update-encryption.js"
  },
