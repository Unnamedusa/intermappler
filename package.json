{
  "name": "intermapper-v8",
  "version": "8.0.0",
  "private": true,
  "description": "INTERMAPPLER v8.0 - Sistema HTA con Acceso Restringido por IP",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:ip": "cross-env PORT=3000 IP_WHITELIST_MODE=strict nodemon server.js",
    "start:custom": "cross-env PORT=3001 IP_WHITELIST_MODE=strict node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "railway up",
    "logs": "railway logs",
    "status": "railway status",
    "connect": "railway connect 3000",
    "myip": "node scripts/get-ip.js",
    "setup": "npm run myip && echo 'âœ… Consulta tu IP en scripts/get-ip.js. Actualiza tu IP en .env si es necesario'",
    "clean": "rm -rf node_modules package-lock.json public/intermapper.hta dist",
    "reinstall": "npm run clean && npm install",
    "security-check": "node scripts/security-check.js",
    "init": "npm install && npm run setup",
    "start:prod": "NODE_ENV=production IP_WHITELIST_MODE=production node server.js",
    "generate-hta": "node scripts/generate-hta.js",
    "generate-env": "node scripts/generate-env.js",
    "backup": "node scripts/backup-system.js",
    "lint": "eslint server.js scripts/",
    "audit": "npm audit --audit-level=high"
  },
  "keywords": [
    "hta",
    "geospatial",
    "mapping",
    "intelligence",
    "security",
    "private-access",
    "ip-restricted",
    "leaflet",
    "plugin-system"
  ],
  "author": "INTERMAPPLER Security Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.0.3",
    "ip": "^2.0.0",
    "geoip-lite": "^1.4.7",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/organization/intermapper-v8-private.git"
  },
  "bugs": {
    "url": "https://github.com/organization/intermapper-v8-private/issues"
  },
  "homepage": "https://github.com/organization/intermapper-v8-private#readme",
  "files": [
    "server.js",
    "scripts/",
    "public/",
    "package.json",
    "README.md",
    ".env.example",
    ".gitignore"
  ],
  "config": {
    "port": 3000,
    "ip_whitelist_mode": "development"
  }
}