{
  "name": "intermappler",
  "version": "3.14.0",
  "description": "Sistema de Mapeo Inteligente Global con Encriptación de 3 Capas y Sistema Multilingüe",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx || true",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,html}\" --ignore-unknown || true",
    "build:assets": "node scripts/build-assets.js",
    "build:translations": "node scripts/generate-translations.js",
    "build": "npm run build:translations && npm run build:assets",
    "clean": "rm -rf node_modules package-lock.json dist build",
    "setup": "node scripts/setup.js",
    "postinstall": "npm run setup && npm run build",
    "dev:frontend": "concurrently \"npm run dev\" \"cd web && npm run dev\"",
    "translate:sync": "node scripts/sync-translations.js",
    "i18n:extract": "node scripts/extract-strings.js"
  },
  "keywords": [
    "intermappler",
    "encryption",
    "security",
    "mapping",
    "real-time",
    "multilingual",
    "translation",
    "i18n"
  ],
  "author": "InterMappler Team",
  "license": "PROPRIETARY",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "crypto-js": "^4.1.1",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.1",
    "mathjs": "^11.11.2",
    "python-shell": "^5.0.0",
    "axios": "^1.6.0",
    "i18next": "^23.7.6",
    "i18next-http-backend": "^2.2.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "node-cache": "^5.1.2",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.16",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intermappler/secure-core.git"
  },
  "bugs": {
    "url": "https://github.com/intermappler/secure-core/issues"
  },
  "homepage": "https://intermappler.org",
  "config": {
    "port": 3000,
    "session_timeout": 86400000,
    "max_file_size": "50mb",
    "encryption_layers": 3,
    "supported_languages": "es,en,fr,de,it,pt,ru,zh,ja,ko,ar,hi",
    "default_language": "es",
    "translation_service": "google"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}