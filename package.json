  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:l1": "nodemon --watch base/incript/L1_quantum.js --exec 'node server.js'",
    "dev:l2": "nodemon --watch base/incript/L2_enigma.js --exec 'node server.js'",
    "dev:l3": "nodemon --watch base/incript/L3_sneaker.js --exec 'node server.js'",
    "dev:all": "nodemon --watch base/incript/*.js --watch server.js --exec 'node server.js'",
    "test": "jest --verbose",
    "test:l1": "jest tests/L1_quantum.test.js --verbose",
    "test:l2": "jest tests/L2_enigma.test.js --verbose",
    "test:l3": "jest tests/L3_sneaker.test.js --verbose",
    "test:security": "node tests/security.test.js",
    "test:translation": "node tests/translation.test.js",
    "test:encryption": "jest encryption.test.js --coverage",
    "test:integration": "jest tests/integration.test.js --verbose",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md,py}\"",
    "security:audit": "npm audit --audit-level=critical",
    "encryption:test": "node scripts/test-encryption.js",
    "encryption:benchmark": "node scripts/benchmark-encryption.js",
    "encryption:keygen": "node scripts/generate-keys.js",
    "encryption:verify": "node scripts/verify-encryption.js",
    "build": "echo 'ðŸ”¨ Building InterMappler v3...' && node scripts/build.js",
