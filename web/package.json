{
  "name": "intermappler-web",
  "version": "3.14.0",
  "description": "Frontend de InterMappler con sistema de traducción multilingüe",
  "private": true,
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:css": "postcss assets/css/**/*.css -d dist/css",
    "extract-translations": "node scripts/extract-strings.js",
    "lint": "eslint . --ext .js || true",
    "format": "prettier --write \"**/*.{js,css,html}\" --ignore-unknown || true"
  },
  "dependencies": {
    "i18next": "^23.7.6",
    "i18next-http-backend": "^2.2.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "postcss-loader": "^7.3.3",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.3",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}